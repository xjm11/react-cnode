{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": "> 1%, last 4 version, not ie <= 8, not dead",
        "useBuiltIns": "usage",
        "corejs": {
          "version": 3.2,
          "proposals": true
        }
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    "react-hot-loader/babel",
    "@babel/plugin-syntax-dynamic-import",
    "@babel/plugin-proposal-object-rest-spread",
    ["@babel/plugin-proposal-class-properties", { "loose": true }],
    [
      "import",
      {
        "libraryName": "antd",
        "libraryDirectory": "es",
        "style": true
      }
    ]
  ]
}
